<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<BorderPane xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.psupozyx.app.LauncherWindow">
   <center>
      <VBox BorderPane.alignment="CENTER">
         <children>
            <HBox spacing="8.0">
               <children>
                  <Button fx:id="launch_graphing_button" mnemonicParsing="false" onAction="#launchGraphing" text="Launch Realtime Graphing" />
                  <Button fx:id="launch_console_button" mnemonicParsing="false" onAction="#launchConsole" text="Launch Console" />
               </children>
               <VBox.margin>
                  <Insets />
               </VBox.margin>
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </padding>
            </HBox>
         </children>
      </VBox>
   </center>
</BorderPane>
